'use strict';require('ts-jest').install();"use strict";
const tslib_1 = require("tslib");
const Program_1 = require("../src/Program");
describe('Program', () => {
    it('passes itself into a command handler', () => tslib_1.__awaiter(this, void 0, void 0, function* () {
        let other;
        const program = new Program_1.Program({
            execute(program) {
                return tslib_1.__awaiter(this, void 0, void 0, function* () {
                    other = program;
                });
            }
        });
        yield program.execute([]);
        expect(other).toBe(program);
    }));
});
